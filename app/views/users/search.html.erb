
<h1>Search</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Usuário</th>
      <th scope="col">Ação</th>
    </tr>
  </thead>
  <tbody>
    <% @searchs.each do |user| %>
      <tr scope="row">
        <td><strong><%= user.username %></strong></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
